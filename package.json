{
    "name": "mitdc-rto-check-in",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "start": "npm run build:css && node ./bin/www",
        "watch": "nodemon --exec npm start",
        "build:css": "postcss app.css -o public/css/style.css",
        "watch:css": "npx tailwindcss -i app.css -o public/css/style.css --watch"
    },
    "dependencies": {
        "autoprefixer": "^10.4.7",
        "cookie-parser": "~1.4.4",
        "debug": "~2.6.9",
        "express": "~4.16.1",
        "express-handlebars": "^6.0.6",
        "express-svelte": "^1.0.8",
        "hbs": "~4.0.4",
        "http-errors": "~1.6.3",
        "moment": "^2.29.3",
        "morgan": "~1.9.1"
    },
    "devDependencies": {
        "cssnano": "^5.1.11",
        "daisyui": "^2.15.2",
        "nodemon": "^2.0.16",
        "postcss-cli": "^9.1.0",
        "tailwindcss": "^3.0.24"
    }
}
